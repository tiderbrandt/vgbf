# SEO and Analytics Configuration
# Copy this to .env.local and update with your values

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://vgbf.se

# Google Analytics (optional)
# Get your Measurement ID from Google Analytics 4
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Umami Analytics (recommended - privacy-focused)
# Get these from your Umami dashboard
NEXT_PUBLIC_UMAMI_URL=https://analytics.umami.is/script.js
NEXT_PUBLIC_UMAMI_WEBSITE_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Privacy Settings
NEXT_PUBLIC_ANALYTICS_ENABLED=true
NEXT_PUBLIC_COOKIES_CONSENT_REQUIRED=true

# SEO Defaults
NEXT_PUBLIC_SITE_NAME="Västra Götalands Bågskytteförbund"
NEXT_PUBLIC_SITE_DESCRIPTION="Information om bågskytte, tävlingar, klubbar och rekord i Västra Götaland."
NEXT_PUBLIC_SITE_KEYWORDS="bågskytte, archery, Västra Götaland, tävling, klubb, rekord, VGBF"

# Social Media (optional)
NEXT_PUBLIC_FACEBOOK_URL=
NEXT_PUBLIC_INSTAGRAM_URL=
NEXT_PUBLIC_YOUTUBE_URL=